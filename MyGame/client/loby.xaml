<Window x:Class="client.loby"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="loby" Height="398" Width="349">
    <Grid>
        <Label Content="Tables List" Margin="100,44,100,0" VerticalAlignment="Top" FontSize="18"/>
        <Button x:Name="btnNewTable" Content="New Table" HorizontalAlignment="Left" Margin="256,7,0,0" VerticalAlignment="Top" Width="75" Click="btnNewTable_Click"/>
        <DataGrid x:Name="tablesData" HorizontalAlignment="Right" Margin="0,83,10,0" VerticalAlignment="Top" Width="321" Height="245" AutoGenerateColumns="True" MouseDoubleClick="tablesData_MouseDoubleClick" IsReadOnly="True"/>
        <Label x:Name="lblName" Content="Label" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Width="122"/>
        <Label x:Name="lblMoney" Content="Label" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Width="106"/>
        <Button x:Name="btnLogout" Content="LogOut" HorizontalAlignment="Left" Margin="256,33,0,0" VerticalAlignment="Top" Width="75" Click="btnLogout_Click"/>

    </Grid>
</Window>
